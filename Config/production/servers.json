{
    "default": {
        "port": "80",
        "host": "0.0.0.0",
        "securityLayer": "none"
    },
    "secure": {
        "port": "443",
        "host": "0.0.0.0",
        "securityLayer": "tls",
        "tls": {
            "certificates": "files",
            "certificateFile": "/private/playground/cert.pem",
            "privateKeyFile": "/private/playground/key.pem",
            "signature": "selfSigned"
        }
    }
}
